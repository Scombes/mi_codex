<h3>Results</h3>
<table class="table library">

        <tr>
          <%= render 'shared/table_head' %>
        </tr>

        <% @search.each do |book| %>
        <tr>
  			     <td><%= book.title %></td>
             <td><%= book.author %></td>
             <td><%= book.publisher %></td>
             <td><%= book.copyright %></td>
             <td><%= book.genre %></td>
             <td><%= book.isbn %></td>
        </tr>
        <% end %>
  	 </table>
      
  		